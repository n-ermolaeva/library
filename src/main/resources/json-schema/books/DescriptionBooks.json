{
  "$schema": "http://json-schema-org/draft-07/schema#",
  "type": "object",
  "javaType": "com.dto.books.DescriptionBooks",
  "description": "Запрос на получение описание книги",
  "required": [
    "bookId",
    "bookName",
    "authorBooksId"
  ],
  "properties": {
    "bookId": {
      "type": "integer",
      "description": "Идентификатор книги"
    },
    "bookName": {
      "type": "string",
      "description": "Название книги",
      "maxLength": 70
    },
    "authorBooksId": {
      "type": "integer",
      "description": "Идентификатор автора"
    }
  }
}

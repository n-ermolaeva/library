{
  "$schema": "http://json-schema-org/draft-07/schema#",
  "type": "object",
  "javaType": "com.dto.books.GetBooksByNameAutors",
  "description": "Запрос на получение списка книг по части имени ее автора",
  "required": [
    "bookName",
    "authorName"
  ],
  "properties": {
    "bookName": {
      "type": "string",
      "description": "Название книги",
      "maxLength": 70
    },
    "authorName": {
      "type": "string",
      "description": "Автор книги",
      "maxLength": 70
    }
  }
}